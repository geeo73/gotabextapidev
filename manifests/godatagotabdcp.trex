    <context-menu>
      <configure-context-menu-item />
    </context-menu>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">GO Data GO Viz Dashboard CP </text>
    </resource>
  </resources>
</manifest>
